<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.midnear.midnearshopping.mapper.delivery.DeliveryInfoMapper">
    <select id="getInvoiceNumberById">
        select invoice_number from delivery_info
        where delivery_id = #{deliveryId}
    </select>
</mapper>